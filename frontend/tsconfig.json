{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/(pages)/Components/loginCallback.js",
    "app/(pages)/friends/page.js",
    "app/(pages)/leaderboard/page.js",
    "app/(pages)/user-profile/[userId]/page.js",
    "app/(pages)/settings/page.tsx"
, "app/(pages)/settings/exaaaample.js", "middleware.tsx"  ],
  "exclude": ["node_modules"]
}
