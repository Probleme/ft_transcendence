  "panels": [
    {
      "title": "Next.js Web Vitals",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 40
      },
      "id": 11,
      "targets": [
        {
          "expr": "nextjs_lcp_value",
          "legendFormat": "Largest Contentful Paint",
          "refId": "A"
        },
        {
          "expr": "nextjs_fid_value",
          "legendFormat": "First Input Delay",
          "refId": "B"
        },
        {
          "expr": "nextjs_cls_value",
          "legendFormat": "Cumulative Layout Shift",
          "refId": "C"
        },
        {
          "expr": "nextjs_ttfb_value",
          "legendFormat": "Time to First Byte",
          "refId": "D"
        }
      ],
      "type": "graph",
      "tooltip": {
        "shared": true,
        "sort": 0,
        "value_type": "individual"
      }
    },
    {
      "title": "Frontend Error Tracking",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 40
      },
      "id": 12,
      "targets": [
        {
          "expr": "rate(nextjs_js_errors_total[5m])",
          "legendFormat": "JS Errors",
          "refId": "A"
        },
        {
          "expr": "rate(nextjs_auth_errors_total[5m])",
          "legendFormat": "Auth Errors",
          "refId": "B"
        },
        {
          "expr": "rate(nextjs_route_change_errors_total[5m])",
          "legendFormat": "Route Errors",
          "refId": "C"
        }
      ],
      "type": "graph"
    },
    {
      "title": "Page Views",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 48
      },
      "id": 13,
      "targets": [
        {
          "expr": "rate(nextjs_page_views_total[5m])",
          "legendFormat": "Views per second",
          "refId": "A"
        }
      ],
      "type": "graph"
    },
    {
      "title": "Frontend Performance Summary",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 48
      },
      "id": 14,
      "type": "stat",
      "targets": [
        {
          "expr": "nextjs_lcp_value",
          "legendFormat": "LCP",
          "refId": "A"
        },
        {
          "expr": "sum(nextjs_js_errors_total)",
          "legendFormat": "Total JS Errors",
          "refId": "B"
        },
        {
          "expr": "sum(nextjs_page_views_total)",
          "legendFormat": "Total Page Views",
          "refId": "C"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      }
    },
    {
      "title": "System Overview",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "panels": [],
      "targets": [
        {
          "expr": "node_memory_MemTotal_bytes - node_memory_MemFree_bytes",
          "legendFormat": "Memory Usage",
          "refId": "A"
        },
        {
          "expr": "rate(node_cpu_seconds_total{mode='user'}[1m])",
          "legendFormat": "CPU Usage",
          "refId": "B"
        },
        {
          "expr": "node_filesystem_avail_bytes",
          "legendFormat": "Disk Space Available",
          "refId": "C"
        }
      ],
      "type": "graph"
    },
    {
      "title": "System Load",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 2,
      "panels": [],
      "targets": [
        {
          "expr": "node_load1",
          "legendFormat": "Load 1m",
          "refId": "A"
        },
        {
          "expr": "node_load5",
          "legendFormat": "Load 5m",
          "refId": "B"
        },
        {
          "expr": "node_load15",
          "legendFormat": "Load 15m",
          "refId": "C"
        }
      ],
      "type": "graph"
    },
    {
      "title": "Django Requests by Method",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "id": 7,
      "panels": [],
      "targets": [
        {
          "expr": "rate(django_http_requests_total_by_method_total[1m])",
          "legendFormat": "{{method}}",
          "refId": "A"
        }
      ],
      "type": "graph"
    },
    {
      "title": "Django Response Status Codes",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "id": 8,
      "panels": [],
      "targets": [
        {
          "expr": "rate(django_http_responses_total_by_status_total[5m])",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "type": "graph"
    },
    {
      "title": "Redis Metrics",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "id": 3,
      "panels": [],
      "targets": [
        {
          "expr": "redis_memory_used_bytes",
          "legendFormat": "Memory Used",
          "refId": "A"
        },
        {
          "expr": "redis_connected_clients",
          "legendFormat": "Connected Clients",
          "refId": "B"
        },
        {
          "expr": "redis_commands_processed_total",
          "legendFormat": "Commands Processed",
          "refId": "C"
        },
        {
          "expr": "redis_keyspace_hits_total",
          "legendFormat": "Keyspace Hits",
          "refId": "D"
        }
      ],
      "type": "graph"
    },
    {
      "title": "PostgreSQL Metrics",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 32
      },
      "id": 5,
      "panels": [],
      "targets": [
        {
          "expr": "pg_stat_database_tup_fetched{datname=~\"$database\"}",
          "legendFormat": "Rows Fetched",
          "refId": "A"
        },
        {
          "expr": "pg_stat_database_numbackends{datname=~\"$database\"}",
          "legendFormat": "Active Connections",
          "refId": "B"
        },
        {
          "expr": "rate(pg_stat_database_xact_commit{datname=~\"$database\"}[5m])",
          "legendFormat": "Transactions/sec",
          "refId": "C"
        },
        {
          "expr": "pg_stat_database_blks_hit_ratio{datname=~\"$database\"}",
          "legendFormat": "Cache Hit Ratio",
          "refId": "D"
        }
      ],
      "type": "graph"
    },
    {
      "title": "PostgreSQL Connection Stats",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 32
      },
      "id": 6,
      "panels": [],
      "targets": [
        {
          "expr": "pg_stat_database_deadlocks{datname=~\"$database\"}",
          "legendFormat": "Deadlocks",
          "refId": "A"
        },
        {
          "expr": "pg_stat_database_conflicts{datname=~\"$database\"}",
          "legendFormat": "Conflicts",
          "refId": "B"
        },
        {
          "expr": "pg_settings_max_connections",
          "legendFormat": "Max Connections",
          "refId": "C"
        }
      ],
      "type": "graph"
    }
  ],
  "refresh": "5s",
  "schemaVersion": 27,
  "style": "dark",
  "tags": [],
  "templating": {
    "list": [
      {
        "name": "database",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(pg_stat_database_tup_fetched, datname)",
        "refresh": 2
      }
    ]
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "title": "ft_transcendence Monitoring Dashboard",
  "version": 1
}